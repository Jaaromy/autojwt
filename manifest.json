{
    "name": "AutoJWT",
    "description": "Request header dev tool",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "activeTab",
        "clipboardWrite",
        "clipboardRead"
    ],
    "background": {
        "scripts": [
            "browser-polyfill.min.js",
            "background.js"
        ]
    },
    "devtools_page": "devtools.html",
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    }
}